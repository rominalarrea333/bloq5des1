class Student:  
    def __init__(self, name, average):  
        self.name = name  
        self.average = average  

def selection_sort(students):  
    n = len(students)  

    for i in range(n):  
        max_index = i  
        for j in range(i + 1, n):  
            if students[j].average > students[max_index].average:  
                max_index = j  
        students[i], students[max_index] = students[max_index], students[i]  

    return students  
